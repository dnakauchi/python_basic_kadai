   "source": [
    "#ランダムな整数を利用するために、randomモジュールをインポート\n",
    "import random\n",
    "\n",
    "# 変数numに0～4までのランダムな整数を代入する\n",
    "num = random.randint(0, 4)\n",
    "\n",
    "# 変数numの値を出力\n",
    "print(num)\n",
    "\n",
    "# 変数numの値が4であれば、「大当たりです」という文字列を出力する\n",
    "if num == 4:\n",
    "    print(\"大当たりです\")\n",
    "else:\n",
    "    print(\"はずれです\")"
   ]
  }
